{
  "name": "simulate-react-cli",
  "version": "1.0.0",
  "description": "simulate create-react-app cli",
  "private": "true",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "eslint": "./node_modules/.bin/eslint src --ext js,jsx",
    "eslint:fix": "./node_modules/.bin/eslint --fix src --ext js,jsx",
    "lint-staged:js": "./node_modules/.bin/eslint --ext .js",
    "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "tslint:fix": "tslint --fix 'src/**/*.{ts,tsx}'",
    "lint": "npm run tslint",
    "lint:fix": "npm run tslint:fix",
    "test": "mocha"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{ts,tsx}": "npm run tslint",
    "**/*.{js,ts,tsx,jsx,scss}": [
      "git add"
    ]
  },
  "keywords": [
    "cli",
    "react"
  ],
  "author": "tdnq",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.1",
    "postcss-scss": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
