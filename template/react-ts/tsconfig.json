{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "jsx": "react"
  },
  "include": [
    "src"
  ]
}